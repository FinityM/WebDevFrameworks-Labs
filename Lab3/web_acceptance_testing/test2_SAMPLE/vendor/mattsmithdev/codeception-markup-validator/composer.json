{
    "name": "mattsmithdev/codeception-markup-validator",
    "version": "4.0.1",
    "description": "Markup validator module for Codeception.",
    "license": "LGPL-3.0-or-later",
    "type": "library",
    "keywords": [
        "acceptance-testing",
        "codeception",
        "codeception-module",
        "html-validator",
        "markup-validator",
        "w3c-validator"
    ],
    "homepage": "https://github.com/dr-matt-smith/codeception-markup-validator",
    "authors": [
        {
            "name": "Matt Smith",
            "email": "dr_matt_smith@me.com",
            "homepage": "http://mattsmithdev.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6 <8.2",
        "codeception/codeception": ">=2.0 <5.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "php-coveralls/php-coveralls": "^2.0",
        "phpunit/phpunit": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Kolyunya\\Codeception\\": "sources"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kolyunya\\Codeception\\Tests\\": "tests"
        }
    }
}
